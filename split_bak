usepeerdns require-mschap-v2 refuse-eap noauth noipdefault

sudo bash -c "route add -host <DNS> -interface ppp0 && route add -net <REMOTE_PC_NETWORK> -interface ppp0 && route add -net 0.0.0.0 -interface en0" && sshpass -p <PASSWORD> ssh -ND7070 <USER>@<IP>& && /Applications/Google\ Chrome.app/Contents/MacOS/Google\ Chrome --proxy-server="socks://localhost:7070" --user-data-dir&
